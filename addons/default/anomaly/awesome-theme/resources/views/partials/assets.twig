{#
    Main CSS can be found in metadata.twig
#}

{# Core Theme Components #}
{{ asset_add("theme.js", "theme::js/vendor/vue.min.js") }}
{{ asset_add("theme.js", "theme::js/vendor/bootstrap.min.js") }}

{# Theme Scripts #}
{{ asset_add("theme.js", "theme::js/plugins/*") }}
{{ asset_add("theme.js", "theme::js/theme/initialize.js") }}
{{ asset_add("stuffs.js", "theme::js/jquery-3.5.1.min.js") }}
{{ asset_add("googlem.js", "theme::js/google-maps.js") }}
{{ asset_add("main_.js", "theme::js/main.js") }}
{{ asset_add("owl.js", "theme::js/vendor/owl-carousel/js/owl.carousel.min.js") }}
{{ asset_add("wow.js", "theme::js/vendor/wow/wow.min.js") }}


{{ asset_script("theme.js", ["min"]) }}
{{ asset_script("stuffs.js") }}
{{ asset_script("googlem.js") }}
{{ asset_script("main_.js") }}
{{ asset_script("owl.js")}}
{{ asset_script("wow.js") }}

{# Include Scripts Loaded From Outside #}
<script type="text/javascript">
    {% for script in asset_inlines("scripts.js", ["min"]) %}
    {{ script|raw }}
    {% endfor %}
</script>

{{ template.includes.render('scripts')|raw }}

{# Include Styles Loaded From Outside #}
<style type="text/css">
    {% for style in asset_inlines("styles.css", ["min"]) %}
    {{ style|raw }}
    {% endfor %}
</style>
